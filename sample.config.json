{
	"input": [
		{
			"type": "tail",
			"filters": ["apache-access-combined"],
			"source": "/var/log/apache2/access_log",
			"data": {
				"host": "example.com"
			}
		}
	],
	"filter": {
		"apache-access-combined": {
			"pattern": "<ipv4> - <user> <time> \"<method> <uri> HTTP\/<httpversion>\" <status> <size> \"<referer>\" \"<useragent>\"",
			"values": {
				"ipv4": "[\\S]+",
				"user": "[\\S]+",
				"time": "\\[[^\\]]+\\]",
				"method": "[a-zA-Z]+",
				"uri": "[\\S]+",
				"httpversion": "[0-9.]+",
				"status": "[0-9]+",
				"size": "[0-9-]+",
				"referer": "[^\"]+",
				"useragent": "[^\"]+"
			},
			"data": {
				"ipv4": "$ipv4",
				"uri": "$uri",
				"status": "$status"
			}
		}
	},
	"output": [
		{
			"type": "hoard",
			"server": "",
			"appkey": "",
			"secret": ""
		}
	]
}